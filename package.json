{
  "name": "nodejs-todo-app",
  "version": "1.0.0",
  "description": "A simple Todo API built with Node.js, Express, TypeScript and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:api": "ts-node src/tests/api.test.ts"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "prisma",
    "todo"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^18.17.0",
    "axios": "^1.8.1",
    "nodemon": "^3.1.9",
    "prisma": "^6.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
